{
  "directory": "~/.i3/i3-resurrect/",
  "window_command_mappings": [
    {
      "class": "Gnome-terminal",
      "command": "gnome-terminal"
    },
    {
      "class": "vesktop",
      "command": "flatpak run dev.vencord.Vesktop"
    },
    {
      "class": "Google-chrome",
      "command": "flatpak run com.google.Chrome"
    },
    {
      "class": "beekeeper-studio",
      "command": "flatpak run io.beekeeperstudio.Studio"
    },
    {
      "class": "Insomnia",
      "command": "flatpak run rest.insomnia.Insomnia"
    }
  ],
  "window_swallow_criteria": {},
  "terminals": [
    "Gnome-terminal",
    "kitty"
  ]
}
